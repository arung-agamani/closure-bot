{
  "name": "closure-bot",
  "version": "1.0.0",
  "description": "Weeb Discord bot for Github event/actions",
  "main": "index.js",
  "scripts": {
    "server": "cross-env BOT='0' SERVER='1' NODE_ENV='prod' ts-node index.ts",
    "server:dev": "cross-env BOT='0' SERVER='1' NODE_ENV='dev' ts-node index.ts",
    "bot": "cross-env BOT='1' SERVER='0' NODE_ENV='prod' ts-node index.ts",
    "bot:dev": "cross-env BOT='1' SERVER='0' NODE_ENV='dev' ts-node index.ts",
    "both": "cross-env BOT='1' SERVER='1' NODE_ENV='prod' ts-node index.ts",
    "both:dev": "cross-env BOT='1' SERVER='1' NODE_ENV='dev' ts-node index.ts",
    "frontend": "yarn run server:dev && yarn run webpack-watch",
    "webpack-watch": "webpack --watch",
    "webpack-build": "webpack --mode-production",
    "webpack-dev": "webpack-dev-server --open",
    "warfarin-build": "webpack --config webpack.warfarin.js",
    "warfarin-watch": "webpack --config webpack.warfarin.js --watch",
    "crx-manifest-build": "tsc -p ./crx/src",
    "crx-popup-build": "webpack --config webpack.warfarin.2.js --env popup",
    "crx-popup-watch": "cross-env NODE_ENV=development webpack --config webpack.warfarin.2.js --env popup --watch",
    "crx-build": "yarn run crx-manifest-build && yarn run crx-popup-build",
    "crx-tailjit": "cross-env NODE_ENV=development postcss -w ./crx/src/popup/**/*.tsx -o ./crx/dist/popup/styles.css",
    "compile": "tsc",
    "reinstall": "rm -rf node_modules",
    "copyenv": "cp .env build/.env && cp views/dist build/views -r",
    "prod": "cd build && cross-env BOT='1' SERVER='1' NODE_ENV='prod' node index.js",
    "build-artifact": "yarn run compile && yarn run copyenv",
    "db-sync": "npx prisma introspect && npx prisma generate"
  },
  "engines": {
    "node": "14.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arung-agamani/closure-bot.git"
  },
  "keywords": [
    "bot",
    "discord",
    "nodejs",
    "javascript",
    "js"
  ],
  "author": "Haruka Shirayuki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arung-agamani/closure-bot/issues"
  },
  "homepage": "https://github.com/arung-agamani/closure-bot#readme",
